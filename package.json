    "@supabase/supabase-js": "^2.49.8",
    "@types/better-sqlite3": "^7.6.13",
    "axios": "^1.8.4",
    "better-sqlite3": "^11.10.0",
    "cli-table3": "^0.6.5",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.5.0",
    "extract-zip": "^2.0.1",
    "googleapis": "^148.0.0",
    "mammoth": "^1.9.0",
    "node-fetch": "^3.3.2",
    "office-text-extractor": "^3.0.3",
    "officeparser": "^5.1.1",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^5.1.91",
    "pptx-parser": "^1.1.7-beta.9",
