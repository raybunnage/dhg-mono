{
  "name": "dhg-monorepo",
  "private": true,
  "scripts": {
    "// Core Development Commands": "Essential turbo commands for development",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:run": "turbo run test:run",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "// Server Utilities": "Development server management",
    "servers": "node scripts/start-all-servers-dynamic.js",
    "servers:legacy": "node scripts/start-all-servers.js",
    "kill-servers": "ts-node scripts/cli-pipeline/servers/commands/kill-all.ts",
    "kill-servers-bash": "bash ./scripts/cli-pipeline/servers/kill-all-servers.sh",
    "browser": "cd html && node server.js",
    "file-browser": "cd html && node server.js",
    "// App Shortcuts": "Quick access to specific app development",
    "hub": "pnpm --filter dhg-hub dev",
    "hub-lovable": "pnpm --filter dhg-hub-lovable dev",
    "audio": "pnpm --filter dhg-audio dev",
    "admin-code": "pnpm --filter dhg-admin-code dev",
    "admin-suite": "pnpm --filter dhg-admin-suite dev",
    "admin-google": "pnpm --filter dhg-admin-google dev",
    "research": "pnpm --filter @dhg/research dev",
    "a": "pnpm --filter dhg-a dev",
    "b": "pnpm --filter dhg-b dev",
    "// Audio-specific Commands": "Audio app development utilities",
    "audio:install": "cd apps/dhg-audio && npm install express googleapis google-auth-library",
    "audio:setup": "node apps/dhg-audio/setup-proxy.js",
    "audio:build": "pnpm --filter dhg-audio build",
    "audio:proxy": "cd apps/dhg-audio && node server.js",
    "audio:start": "pnpm run audio:setup && pnpm run audio:build && pnpm run audio:proxy",
    "// CLI Pipeline Commands": "Quick access to CLI pipeline tools",
    "google": "./scripts/cli-pipeline/google_sync/google-sync-cli.sh",
    "doc": "./scripts/cli-pipeline/document/doc-cli.sh",
    "classify": "./scripts/cli-pipeline/classify/classify-cli.sh",
    "media": "./scripts/cli-pipeline/media-processing/media-processing-cli.sh",
    "presentations": "./scripts/cli-pipeline/presentations/presentations-cli.sh",
    "prompt": "./scripts/cli-pipeline/prompt_service/prompt-service-cli.sh",
    "ai": "./scripts/cli-pipeline/ai/ai-cli.sh",
    "auth": "./scripts/cli-pipeline/auth/auth-cli.sh",
    "database": "./scripts/cli-pipeline/database/database-cli.sh",
    "email": "./scripts/cli-pipeline/email/email-cli.sh",
    "experts-cli": "./scripts/cli-pipeline/experts/experts-cli.sh",
    "tasks": "./scripts/cli-pipeline/dev_tasks/dev-tasks-cli.sh",
    "merge": "./scripts/cli-pipeline/merge/merge-cli.sh",
    "monitoring": "./scripts/cli-pipeline/monitoring/monitoring-cli.sh",
    "scripts": "./scripts/cli-pipeline/scripts/scripts-cli.sh",
    "tracking": "./scripts/cli-pipeline/tracking/tracking-cli.sh",
    "worktree": "./scripts/cli-pipeline/worktree/worktree-cli.sh",
    "all": "./scripts/cli-pipeline/all_pipelines/all-pipelines-cli.sh",
    "maintenance": "./scripts/cli-pipeline/maintenance-cli.sh",
    "service-deps": "./scripts/cli-pipeline/service_dependencies/service-dependencies-cli.sh",
    "// Utility Commands": "Helpful utilities",
    "tree": "node scripts/show-tree.js",
    "types": "supabase gen types typescript --project-id jdksnfkupzywjdfefkyj > supabase/types.ts",
    "// Git & Promotion": "Use git-workflow-cli.sh for comprehensive git operations",
    "git": "./scripts/cli-pipeline/git_workflow/git-workflow-cli.sh"
  },
  "bin": {
    "dhg-tree": "./scripts/show-tree.js"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.1",
