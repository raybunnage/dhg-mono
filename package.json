    "supabase": "2.12.1",
    "turbo": "^1.12.4"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "axios": "^1.8.4",
    "dotenv": "^16.4.7",
    "extract-zip": "^2.0.1",
    "googleapis": "^148.0.0",
    "mammoth": "^1.9.0",
    "node-fetch": "^3.3.2",
    "office-text-extractor": "^3.0.3",
